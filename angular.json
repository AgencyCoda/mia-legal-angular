{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-legal": {
      "projectType": "library",
      "root": "projects/agencycoda/mia-legal",
      "sourceRoot": "projects/agencycoda/mia-legal/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/agencycoda/mia-legal/tsconfig.lib.json",
            "project": "projects/agencycoda/mia-legal/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-legal/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-legal/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-legal/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-legal/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/agencycoda/mia-legal/tsconfig.lib.json",
              "projects/agencycoda/mia-legal/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "26435992-420b-463d-8e75-631b32f04e17"
  },
  "defaultProject": "@agencycoda/mia-legal"
}